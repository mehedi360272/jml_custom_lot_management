<odoo>
    <record id="view_mrp_warning_wizard_form" model="ir.ui.view">
        <field name="name">mrp.warning.wizard.form</field>
        <field name="model">mrp.warning.wizard</field>
        <field name="arch" type="xml">
            <form string="Warning">
                <sheet>
                    <group>
                        <field name="warning_message" nolabel="1" readonly="1"/>
                    </group>
                    <footer>
                        <button string="Confirm  Produce" type="object" name="action_confirm" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
